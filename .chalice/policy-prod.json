{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:*",
        "lambda:*",
        "sqs:*",
        "es:*",
        "rds:*",
        "ec2:*",
        "ecs:*",
        "logs:*",
        "cloudformation:*",
        "cloudwatch:*",
        "autoscaling:*",
        "iam:PassRole",
        "elasticloadbalancing:*",
        "states:*",
        "dynamodb:*"
      ],
      "Resource": [
        "*"
      ],
      "Effect": "Allow",
      "Sid": "bf900f21bc4349b19c4ad59b62fade1b"
    },
    {
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*",
      "Effect": "Allow"
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "ec2:CreateNetworkInterface",
        "ec2:DescribeNetworkInterfaces",
        "ec2:DeleteNetworkInterface",
        "ec2:AssignPrivateIpAddresses",
        "ec2:UnassignPrivateIpAddresses"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Encrypt",
        "kms:Decrypt",
        "kms:ReEncrypt*",
        "kms:GenerateDataKey*",
        "kms:DescribeKey"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}
